{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "esnext",
    "skipLibCheck": true,
    "types": ["vite/client", "vitest/globals", "./types/prop-types"],
    "paths": {
      "prop-types": ["types/prop-types/index.d.ts"],
      "prop-types/*": ["types/prop-types/index.d.ts"],
      "prop-types/factoryWithTypeCheckers": ["types/prop-types/factoryWithTypeCheckers.d.ts"],
      "prop-types/factoryWithTypeCheckers.js": ["types/prop-types/factoryWithTypeCheckers.d.ts"]
    }
  },
  "include": [
    "src/**/*",
    "api/**/*"
  ]
}
